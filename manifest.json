{
    "manifest_version" : 2,

    "author" : "madcamp",
    "name" : "Cat Dominates World",
    "description": "Cat will dominate the world some time.",
    "version" : "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "icons": {
            "128": "icon.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "run_at": "document_idle",
        "js": ["content.js"]
      }],


    "permissions": ["tabs", "<all_urls>"]
}